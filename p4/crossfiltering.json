{
    "data" : {
        "source": "data/synthetic",
        "format": "typedArray",
        "schema": {
            "BirthMonth"   : "int",
            "BabyGender"   : "string",
            "BabyWeight"   : "float",
            "MotherAge"    : "int",
            "MotherRace"   : "string",
            "MotherStatus" : "string",
            "MotherEdu"    : "string",
            "MotherHeight" : "float",
            "MotherWeight" : "float",
            "MotherWgtGain": "float"
        }
    },
    "views": [
        {
            "id": "chart1",
            "width": 360,
            "height": 360,
            "padding": {"left": 80, "right": 20, "top": 20, "bottom": 100},
            "offset": [0, 100]
        },
        {
            "id": "chart2",
            "width": 540,
            "height": 360,
            "padding": {"left": 80, "right": 20, "top": 20, "bottom": 100},
            "offset": [330, 100]
        }
    ],
    "pipeline": [
        {
            "$select": {
                "MotherAge": [22, 24],
                "FatherAge": [22, 24]
            }
        },
        {
            "$visualize": {
                "id": "chart1",
                "mark": "circle",
                "size": 15,
                "color": "red",
                "opacity": 0.25,
                "x": "MotherWeight",
                "y": "BabyWeight",
                "brush": 1
            }
        },
        {
            "$visualize": {
                "id": "chart2",
                "mark": "line",
                "color": "red",
                "opacity": 0.1,
                "y": [
                    "BabyWeight",
                    "MotherHeight",
                    "MotherWeight",
                    "MotherWgtGain"
                ],
                "brush": 1
            }
        }
    ]
}
